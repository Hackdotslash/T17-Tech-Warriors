{
        "manifest_version": 2, 
        "version": "1.0",
        "name": "Cyber Creepers",
        "description": "!!",
        "icons": {
        "16": "images/whatsapp.png",
        "48": "images/whatsapp.png",
        "128": "images/whatsapp.png"
        },
        "browser_action": {
        "default_icon": "images/whatsapp.png",
        "default_popup": "popup.html"
        },
        "permissions": [
        "activeTab",
        "tabs"
        ],
        "background": {
                "scripts": [
                        "index.js"
                ],
                "persistent": false
        },
        "content_scripts": [
                {
                        "match_about_blank": true,
                        "matches": [
                                "<all_urls>"
                        ],
                        "js": [
                                "index.js"
                        ]
                }
        ]
}
